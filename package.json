{
  "name": "@3fv/ditsy",
  "version": "1.0.14",
  "description": "Extended from `async-injection` originally authored by Frank Stock",
  "author": "Jonathan Glanz",
  "license": "MIT",
  "keywords": [
    "ioc",
    "di",
    "async",
    "dependency",
    "injection",
    "dependency injection",
    "inversion of control",
    "injector",
    "container",
    "typescript"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "/README.md",
    "/License.txt",
    "/package.json",
    "/lib"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "fullclean": "yarn run clean && rimraf ./package-lock.json && rimraf ./node_modules",
    "dev": "concurrently 'npm:build:watch' 'npm:test:watch'",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test --watchAll",
    "test:watch:debug": "node --inspect node_modules/.bin/jest  --runInBand --watchAll",
    "build": "yarn run clean && tsc -b tsconfig.json",
    "build:watch": "yarn run build --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3fv/ditsy.git"
  },
  "bugs": {
    "url": "https://github.com/3fv/ditsy/issues"
  },
  "dependencies": {
    "@3fv/deferred": "^1.2.14",
    "@3fv/guard": "^1.4.39",
    "@3fv/prelude-ts": "^0.8.37",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "ts-pattern": "^3.3.5"
  },
  "devDependencies": {
    "@swc/core": "1.2.118",
    "@swc/helpers": "^0.3.8",
    "@swc/jest": "0.2.11",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^16.11.22",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-package-json": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.11.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.3",
    "ts-lint": "^4.5.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@swc/core": "1.2.118",
    "@swc/jest": "0.2.11",
    "react-refresh": "0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "jest": "27.5.1",
    "lodash": "^4.17.21",
    "ansi-regex": "^5.0.1",
    "tmpl": "^1.0.5",
    "path-parse": "^1.0.7",
    "hosted-git-info": "^2.8.9",
    "minimist": "^0.2.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true
  }
}
